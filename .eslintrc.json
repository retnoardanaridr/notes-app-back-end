{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12 
    },
    "rules": {
        "no-console" : "off",
        "linebreak-style" : "off",
        "indent" : "off",
        "eol-last" : "off",
        "no-unused-vars": "off",
        "spaced-comment": "off"
    }
}
